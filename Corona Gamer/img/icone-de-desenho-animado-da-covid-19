<script>

function loadPayPalData(){

    var idPlan = $("#paypal_form input[name=idPlan]").val();
    var ab_main_url = "";
    var ab_template = "";
    var ab_option = "";
    var country_code = "";
    var country_name = "";

   if(globalUserStatusLoaded){
        if(globalABMainURL != "null" && globalABMainURL != null && globalABMainURL != ""){
            ab_main_url = globalABMainURL;
            ab_template = globalABTemplate;
            ab_option = globalABOption;
        } else{
            ab_main_url = "not set";
            ab_template = "not set";
            ab_option = "not set";
        }  
        country_code = globalUserCountryCode; 
        country_name = globalUserCountryName;            
    } else{
        console.log("waiting for loadPayPalData");
        setTimeout(loadPayPalData, loopInterval);
        return;
    }

    var custom = "idPlan:" + idPlan +
                    "@idVexelsUser:" + $("#paypal_form input[name=idUser]").val() +
                    "@isSubscription:"  + $("#paypal_form input[name=isSubscription]").val() + 
                    "@redirectVector:"  + $("#paypal_form input[name=redirectVector]").val() +
                    "@idVector:" + $("#paypal_form input[name=idVector]").val()  +
                    "@cjCookie:" + ((typeof $.cookie("cje") === 'undefined' )?'': $.cookie("cje")) + 
                    "@lang:pt" +
                    "@country_code:" + country_code + 
                    "@country_name:" + country_name +
                    "@ab_main_url:" + ab_main_url + 
                    "@ab_template:" + ab_template + 
                    "@ab_option:" + ab_option + 
                    "@idUser_aux:";

    //console.log("Custom PayPal field: " + custom);

    $("#paypal_form input[name=id_user]").val($("#paypal_form input[name=idUser]").val());
    $("#paypal_form input[name=custom]").val(custom);
    $("#paypal_form input[name=item_number]").val($("#paypal_form input[name=idVector]").val());

    if($("#paypal_form input[name=idVector]").val() != "" && $("#paypal_form input[name=idVector]").val() != "undefined"){
        $(".individual_licence").show();
        $("#item_name_paypal").html($("#paypal_form input[name=vectorName]").val().replace(/_/g, ' ').toUpperCase());
        $("#paypal_form input[name=item_name]").val($("#paypal_form input[name=vectorName]").val().replace(/_/g, ' ').toUpperCase() + " License");
    } else{
        $(".individual_licence").hide();
        $("#item_name_paypal").html('');
        $("#paypal_form input[name=item_name]").val($("#plan_name_paypal").html());
    }

    $("#paypal_form input[name=cmd]").val('_s-xclick'); 

    switch (idPlan){
        
            case "2":  
            		$("#paypal_form input[name=hosted_button_id]").val('FWHSEAPB9BGBQ');//one_time_5
            		break;
        
            case "3":  
            		$("#paypal_form input[name=hosted_button_id]").val('JFJYFBZHHXSQG');//premium
            		break;
        
            case "4":  
            		$("#paypal_form input[name=hosted_button_id]").val('6BJ35RZB3Y4EE');//extended
            		break;
        
            case "5":  
            		$("#paypal_form input[name=hosted_button_id]").val('TVC7Q2TCMCHV8');//new_anual_premium
            		break;
        
            case "6":  
            		$("#paypal_form input[name=hosted_button_id]").val('NCKA7YBEYPN9U');//new_anual_extended
            		break;
        
            case "7":  
            		$("#paypal_form input[name=hosted_button_id]").val('WWEPAJGXKA57L');//one_time_1
            		break;
        
            case "8":  
            		$("#paypal_form input[name=hosted_button_id]").val('UPK9CCWBGH2FU');//individual_extended
            		break;
        
            case "9":  
            		$("#paypal_form input[name=hosted_button_id]").val('ZLT4A5K235DD6');//ind_isolated_extended
            		break;
        
            case "13":  
            		$("#paypal_form input[name=hosted_button_id]").val('RKXFVBV4ZTJ5Y');//merch
            		break;
        
            case "14":  
            		$("#paypal_form input[name=hosted_button_id]").val('TF2QZ67W3AN7U');//annual_merch
            		break;
        
            case "22":  
            		$("#paypal_form input[name=hosted_button_id]").val('6SY5KZB2UAS4A');//chr_2018_anual_premium
            		break;
        
            case "23":  
            		$("#paypal_form input[name=hosted_button_id]").val('SXJE9QY2AME74');//chr_2018_anual_extended
            		break;
        
            case "24":  
            		$("#paypal_form input[name=hosted_button_id]").val('TBPHAQ9PFNP7Q');//chr_2018_annual_merch
            		break;
        
            case "25":  
            		$("#paypal_form input[name=hosted_button_id]").val('WSU393Z7MGGFS');//merch_lifetime
            		break;
        
            case "26":  
            		$("#paypal_form input[name=hosted_button_id]").val('MC6PLYPJ736X6');//business_merch_lifetime
            		break;
        
            case "31":  
            		$("#paypal_form input[name=hosted_button_id]").val('RAZWYDKBTA5QL');//merch_xs
            		break;
        
            case "32":  
            		$("#paypal_form input[name=hosted_button_id]").val('WF2L3GZ24TLUU');//annual_merch_xs
            		break;
        
        default:
            console.log("error paypal");
    }

    if($("#paypal_form input[name=idUser]").val() == "" || $("#paypal_form input[name=idUser]").val() == "0"){
        displayGlobalMessage("Error - please contact Admin.", "");
    }  
}

function goTo_paypal(){
    if($("#paypal_form input[name=custom]").val() == "" || $("#paypal_form input[name=id_user]").val() == "0" || $("#paypal_form input[name=id_user]").val() == ""){
        displayGlobalMessage("Error - please contact Admin.", "");
    } else{
        console.log(JSON.stringify($("#paypal_form").serialize()));
        $("#paypal_form").submit();       
    } 
}

function goTo_bluesnap(){
    processPayment('', '', '');
}

var validating = false;

function toggleValidatingBlur(){
    if(!validating){
        $(".validando-tarjeta").fadeIn('500');
        validating = true;
        $("#validator__aux_div").addClass('validando-tarjeta-fondo');
    } else{
        $(".validando-tarjeta").fadeOut('1500');
        validating = false;
        $("#validator__aux_div").removeClass('validando-tarjeta-fondo');
    }
}

function showConfirmPurchasePanel(type){

    var text = type=='paypal'?"You'll be redirected to Paypal. Confirm purchase?":"You order will be processed. Confirm Purchase?";
    var panel = getConfirmPanelHTML(type, text, "goTo_" + type + "();", "cancelPurchase('" + type + "');");
    
    setTimeout("goTo_" + type + "();", 0);
}

function cancelPurchase(type){
    
    $("#confirm_panel_" + type).fadeOut('1500', function(){
        if(type == "paypal"){
            $("#paypal_button").fadeIn('1500');
        } else{
            $("#bluesnap_button").fadeIn('1500');        
        }
        $("#confirm_panel_" + type).remove();
    });
}

function getConfirmPanelHTML(type, question_text, yes_action, no_action){

    var html = "";

    html += '<div id="confirm_panel_' + type + '" class="confirm_purchase_panel">';
    html += '   <div class="confirm__purchase_text">' + question_text + '</div>';
    html += '   <div class="confirm_buttons_container">';
    html += '       <a class="confirm__purchase_yes_btn clickable" style="display:none;" id="yes_button_' + type + '" onclick="' + yes_action + '">Yes</a>';
    html += '       <div class="confirm__or">or</div>';
    html += '       <a class="confirm__purchase_no_btn clickable" style="display:none;" id="no_button_' + type + '" onclick="' + no_action + '">No</a>';
    html += '   </div>';
    html += '</div>';

    return html;
}

function dismissPrompt(obj){
     $(obj).fadeOut(function(){
        $('#no_more_label').fadeOut();
        dismissPendingNotification('prompt_notification', true);
     });
}

</script>

