function loadRecaptcha(){
    grecaptcha.ready(function() {
        grecaptcha.execute(secretKey, {action: 'homepage'}).then(function(token) {
           //console.log("New Recaptcha Loaded With Token: " + token);
           $(".g-recaptcha-response").val(token);
        });
    });
}

