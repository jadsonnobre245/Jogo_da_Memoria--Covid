<script type="text/javascript">

    var downloadedVectors = "";
    var downloadLimitDate = "";
    var proPlanAlreadyGot = false;
    
    $(document).ready(function(){
        $(".dinamic_code_field_modal").val($("#dinamic_code_holder").val());  
        
        getIndividualPlanByName();

        // prevent right click
        $('.image-precio').bind("contextmenu", function(e) {
            if (!globalUserIsSubscriptor) {
            return false;
            }
        });

        $("#load_bar_download_free").hide();
        $("#chk_free_download").hide();
        $("#lbl_chk_free_download").hide();
        $("#free_download_link").hide();
        
        checkOnlyMerchModal();
    });

    function checkOnlyMerchModal(){
        consoleLog("Running checkOnlyMerchModal");
        if(globalUserStatusLoaded){
            consoleLog("setting setMerchPlanPrice");
            if(globalIsCurrentVectorMerchOnly){
                setMerchPlanPrice();
            }
            else{
                if(proPlanAlreadyGot === false) setProPlanPrice();
            }
        } else{
            consoleLog("waiting for checkOnlyMerchModal");
            setTimeout(checkOnlyMerchModal, loopInterval);
        } 
    }

    function setMerchPlanPrice(){

        consoleLog("Running setMerchPlanPrice");

        $.ajax({
            url:    '/ajax/vexels-api/get-id-plan-by-name/',
            type:     'post',
            data:     {planName: "annual_merch"},            
            beforeSend: function(request) {
                request.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
            },
            statusCode: {
                    404: function() {
                    consoleLog('Could not contact server.');
                    },
                    500: function() {
                    consoleLog('A server-side error has occurred.');
                    }
            },
            success:  function(data) {
                consoleLog(JSON.stringify(data)); 

                var price_arr = data.plan_cost.split(".");

                var anualMerchPlanCost  = Math.round(parseFloat(data.plan_cost) * 100) / 100;
                var monthAnualMerch     = Math.round(Math.ceil(anualMerchPlanCost / 12) * 100) / 100;
                var price_arr           = monthAnualMerch.toString().split(".");

                consoleLog(price_arr);

                $(".text-suscribe-6").html(price_arr[0]);
                
                consoleLog(price_arr[1]);
                if(price_arr[1] != "" && price_arr[1] !== undefined){
                    $(".text-suscribe-99").html("." + price_arr[1]);
                } else{
                    $(".text-suscribe-99").html("");
                }

                consoleLog("loaded plan: MERCH");
                consoleLog("MERCH price: " + data.plan_cost);
            },
            fail:     function(error){
                console.info(JSON.stringify(error));
            }
        });
    }

    function setProPlanPrice(){

        consoleLog("Running setProPlanPrice");

        $.ajax({
            url:    '/ajax/vexels-api/get-id-plan-by-name/',
            type:     'post',
            data:     {planName: "new_anual_premium"},        
            beforeSend: function(request) {
                request.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
            },
            statusCode: {
                    404: function() {
                    consoleLog('Could not contact server.');
                    },
                    500: function() {
                    consoleLog('A server-side error has occurred.');
                    }
            },
            success:  function(data) {
                consoleLog(JSON.stringify(data)); 

                var price_arr = data.plan_cost.split(".");

                var anualProPlanCost    = Math.round(parseFloat(data.plan_cost) * 100) / 100;
                var monthAnualPro       = Math.round((anualProPlanCost / 12) * 100) / 100;
                var price_arr           = monthAnualPro.toString().split(".");

                consoleLog(price_arr);

                $(".text-suscribe-6").html(price_arr[0]);

                consoleLog(price_arr[1]);     
                if(price_arr[1] != "" && price_arr[1] !== undefined){
                    if(price_arr[1]<10) price_arr[1] = price_arr[1].toString() + "0";
                    $(".text-suscribe-99").html("." + price_arr[1]);
                } else{
                    $(".text-suscribe-99").html("");
                }

                consoleLog("loaded plan: PRO");
                consoleLog("PRO price: " + data.plan_cost);
                proPlanAlreadyGot = true;
                checkOnlyMerchModal();
            },
            fail:     function(error){
                console.info(JSON.stringify(error));
            }
        });
    }

    function getIndividualPlanByName(){
        consoleLog("Runnig getIndividualPlanByName");

        $.ajax({
            url:    '/ajax/vexels-api/get-id-plan-by-name/',
            type:     'post',
            data:     {planName: planName},
            beforeSend: function(request) {
                request.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
            },
            statusCode: {
                    404: function() {
                    if(common_debug_console) consoleLog('Could not contact server.');
                    },
                    500: function() {
                    if(common_debug_console) consoleLog('A server-side error has occurred.');
                    }
            },
            success:  function(data) {
                //consoleLog(JSON.stringify(data)); 
                $("#ind_ext_check").data("planex", data.plan_id);
                $("#ind_ext_check").data("planexprice", data.plan_cost);
                consoleLog($("#ind_ext_check").data("planex"));
                consoleLog($("#ind_ext_check").data("planexprice"));

                checkPremium('193297');
            },
            fail:     function(error){
                console.info(JSON.stringify(error));
            }
        });
    }

    function freeDownloadAction(){
        if($("#chk_free_download").is(":checked")){
            $("#chk_free_download").hide();
            $("#lbl_chk_free_download").hide();
            $("#load_bar_download_free").fadeIn(function(){
                $("#free_download_link").show();
                $("#load_bar_download_free").hide();

                if("7" == "7"){
                    $("#free_download_link").removeAttr("href");
                    $("#free_download_link").on("click",function(){
                        $("#formFree").submit();
                    });
                } 
            });                     
        }
    }

    function checkFreeDownload(){

        if("7" == "7"){
            var element = "isolated_download";
        } else{
            var element = "url_download";
        }

        $("#" + element).fadeOut(function(){
            $("#lbl_chk_free_download").show();
            $("#chk_free_download").fadeIn(function(){
                $("#load_bar_download_free").hide();
            });
        });
    }

    function checkPremium(idVector){
        consoleLog("running checkPremium");
        if(globalUserStatusLoaded){
            if(globalIdCurentVector != "" && globalIsCurrentVectorMerchOnly && globalUserPlanType != "MERCH" && globalUserPlanType != "EXT"){
        
                consoleLog("cambiando 5 por 39");

                $("#not_premium_vector").hide();
                $("#premium_vector").hide();
                $("#merch_vector").show();
                $("#get_text_merch").show();
                $("#get_text_pro").hide();

                $("#subscribe-btn-download").attr("href", $("#subscribe-btn-download").attr("href").replace("/plans/","merch-plans/"));
                $("#more-pro").attr("href", $("#more-pro").attr("href").replace("/plans/","merch-plans/"));
                $("#subscribe-now-merch").attr("href", $("#subscribe-now-merch").attr("href").replace("/plans/","merch-plans/"));
                $("#check-plans-merch").attr("href", $("#check-plans-merch").attr("href").replace("/plans/","merch-plans/"));

                $("#ind_ext_check").prop('checked', true);
                $("#ind_ext_check").click();
                $("#ind_ext_check").parent().hide();
            } else if(globalIdCurentVector != "" && globalIsCurrentVectorPremium){
                $("#not_premium_vector").hide();
                $("#merch_vector").hide();
                $("#premium_vector").show();
                $("#get_text_merch").hide();
                $("#get_text_pro").show();
            } else {
                $("#not_premium_vector").show();
                $("#premium_vector").hide();
                $("#merch_vector").hide();
                $("#get_text_merch").hide();
                $("#get_text_pro").show();
                
                $("#free_download_limit_reached").hide();
                $("#free_download_available").hide(); 
                
                downloadedVectors = checkDownload();

                if (Number(downloadedVectors) < free_download_limit){
                    $("#free_download_available").show(); 
                } else {
                    $("#free_download_limit_reached").show();
                    setAfterLoginJS("afterLoginDownload", "");
                }
            }
        } else {
            setTimeout(function(){
                checkPremium(idVector);
            }, loopInterval);
        }
    }

    function clickDownload(){
        if (localStorage.getItem("downloadedVectors") === null) {
            downloadedVectors = 0;
        } else {
            downloadedVectors++;
        }
        localStorage.setItem("downloadedVectors", downloadedVectors);
    }

    function checkDownload(){
        var dateObj = new Date();
        var month = dateObj.getUTCMonth() + 1; //months from 1-12
        var day = dateObj.getUTCDate();
        var year = dateObj.getUTCFullYear();
        var dateToday = year + "/" + month + "/" + day;
        
        if (localStorage.getItem("downloadedVectors") === null) {
            localStorage.setItem("downloadLimitDate", dateToday);
            downloadLimitDate = localStorage.getItem("downloadLimitDate");
            localStorage.setItem("downloadedVectors", 0);
        } else {
            downloadLimitDate = localStorage.getItem("downloadLimitDate");
            if (dateToday === downloadLimitDate){
                downloadedVectors = localStorage.getItem("downloadedVectors");
            } else {
                localStorage.setItem("downloadedVectors", 0);
                localStorage.setItem("downloadLimitDate", dateToday);
                downloadedVectors = 0;
            }
        }
        return downloadedVectors;
    }

    function afterLoginDownload(){
        setAfterLoginJS("", "");
        window.location.href='/download-png-svg/193297/-cone-de-desenho-animado-da-covid-19';
    }

</script>


