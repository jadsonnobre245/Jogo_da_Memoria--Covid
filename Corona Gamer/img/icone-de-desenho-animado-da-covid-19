<script>
    window.fbAsyncInit = function() {
      FB.Event.subscribe('auth.authResponseChange', function(response) {
        if (response.status === 'connected') {
          consoleLog("conected");
        } else if (response.status === 'not_authorized') {
        } else {
            FB.login(function(response) {
         }, {scope: 'email,user_likes'});
        }
      });
    };

    
    function consultAPI() {
      FB.api('/me', function(response) {
       var lang = $('input[name="lang"]').val();
        var data = {'user_id' : 1, 'id_facebook':response.id , 'firstname': response.first_name, 'lastname': response.last_name, 'email': response.email, 'username':response.email, 'lang':lang};
        if($('input[name="signfree"]').length > 0){
          data.signfree = $('input[name="signfree"]').val();
        }
        
        var id_facebook = response.id;
        showLoading();
        if(!loginFacebook(id_facebook)){
          $.post('/ajax/signup/', data, function(resp){
            try{
              if(resp.res == -1){
                message("Message", resp.msg,"");
                hideLoading();
              }else{
                if(resp.url != null )
                  window.location.href = resp.url;
                else{
                  window.location.href = "/";
                }         
              }
            }catch(e){consoleLog("catch");}
          }); 
        }
      });
    }
  function registryFB(){
    FB.getLoginStatus(function(response) {
        if (response.authResponse) {
            consultAPI();
        } else {
             FB.login(function(response) {
         }, {scope: 'email,user_likes'});
        }
    });

     
  }
  </script>

