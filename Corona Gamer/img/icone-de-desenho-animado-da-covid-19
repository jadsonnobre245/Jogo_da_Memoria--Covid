            <script type="text/javascript">
                function doSignup(e){

                    e.stopPropagation();
                    e.stopImmediatePropagation();
                    e.preventDefault();
                    console.log("Re Captcha:");
                    console.log(grecaptcha.getResponse());

                    var captchaResponse = grecaptcha.getResponse($("#" + $(this).data("recaptcha-field-id")).data("widget-id"));

                    if(captchaResponse != ""){
                        consoleLog("Recaptcha OK");
                    } else{
                        consoleLog("Recaptcha NOT OK");
                        displayGlobalMessage("Por favor, confirme que você é um ser humano, clicando no Recaptcha", "");
                        return false;
                    }

                    var formID = $(this).data("form-id");

                    if($("#" + formID + " input[name=username_join]").val()==""){
                        displayGlobalMessage("Wrong Username", "");
                    } else if($("#" + formID + "  input[name=pass]").val()==""){
                        displayGlobalMessage("Password Error", "");
                    } else{
                        $("#" + formID + "").submit();
                    }
                }
            </script> 
