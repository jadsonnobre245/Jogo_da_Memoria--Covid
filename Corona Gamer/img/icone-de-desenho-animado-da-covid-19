        function facebookLogin______() {
           FB.getLoginStatus(function(response) {
              if (response.authResponse) {
                 login();
              } else {
                    FB.login(function(response){
                      scope: 'email,user_birthday,status_update,publish_stream' 
                  });
              }
          });
           
        }
        
       function login_____(){
           FB.api('/me', function(response) {
                var data = {'user_id' : 1, 'id_facebook':response.id , 'firstname': response.first_name, 'lastname': response.last_name, 'email': response.email, 'username':response.email};
                var id_facebook = response.id;
                if(!loginFacebook(id_facebook)){
                    message("Message", "Error Login","");
                }
           });
       }
       function loginFacebook______(id){
          $.ajaxSetup({async:false});
          var login = true;
          showLoading();
          $.post('/ajax/login/', {'user_id':id, 'type_user_id':'facebook'}, function(resp){
            hideLoading();
              resp = $.parseJSON(resp);
                if(resp.res != -1){
                  location.reload();
                  $.ajaxSetup({async:true});
                }else{
                  login = false;
                }
          }); 
          $.ajaxSetup({async:true});
          return login;
        }
</script>
